{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build tackettweb",
            "type": "shell",
            "command": "npm run build",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "dev tackettweb",
            "type": "shell",
            "command": "npm run dev",
            "group": {
                "kind": "test",
                "isDefault": true
            }
        },
        {
            "label": "deploy tackettweb",
            "type": "shell",
            "command": "vercel --prod",
            "group": "none"
        }
    ]
}